{
  "name": "d3bu.net",
  "version": "2.3.0",
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "5.x",
    "@fortawesome/free-regular-svg-icons": "5.x",
    "@fortawesome/free-solid-svg-icons": "5.x",
    "katex": "0.16.4",
    "sanitize.css": "13.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "1.0.0",
    "@sveltejs/adapter-static": "1.0.0",
    "@sveltejs/kit": "1.0.1",
    "@vitejs/plugin-legacy": "3.0.1",
    "cssnano": "5.1.14",
    "cssnano-preset-advanced": "5.3.9",
    "dotenv-cli": "6.0.0",
    "js-beautify": "1.14.7",
    "mdsvex": "0.10.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.20",
    "postcss-load-config": "4.0.1",
    "postcss-viewport-height-correction": "1.1.1",
    "rehype-katex-svelte": "1.1.2",
    "remark-math": "3.x",
    "sass": "1.57.1",
    "svelte": "3.55.0",
    "svelte-awesome": "3.0.1",
    "svelte-check": "3.0.1",
    "svelte-preprocess": "5.0.0",
    "svelte-sitemap": "2.5.4",
    "terser": "5.16.1",
    "tiny-glob": "0.2.9",
    "tslib": "2.4.1",
    "typescript": "4.9.4",
    "vite": "4.0.3"
  },
  "packageManager": "pnpm@8.15.7",
  "scripts": {
    "build": "dotenv run-s stage:kit stage:404 stage:sitemap stage:beautify",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check-watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "dotenv vite dev",
    "dev-host": "dotenv vite dev --host",
    "preview": "dotenv vite preview",
    "preview-host": "dotenv vite preview --host",
    "ssr:build": "SSR='true' run-s stage:kit",
    "ssr:start": "node build",
    "stage:404": "mv build/404/index.html build/404.html && rmdir build/404",
    "stage:beautify": "node beautify",
    "stage:kit": "vite build",
    "stage:old": "mkdir -p static/old && run-p 'stage:old:*'",
    "stage:old:v0": "cp -r old/v0 static/old/v0 && cp -r old/v0-blog static/old/v0/Blog",
    "stage:old:v1": "npm run -w old/v1 build && cp -r old/v1/public static/old/v1",
    "stage:sitemap": "svelte-sitemap -d https://d3bu.net -i '404.html' -i 'inner/**'"
  },
  "type": "module",
  "workspaces": [
    "old/v1"
  ]
}
