{
  "name": "d3bu.net",
  "version": "2.1.0",
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "5.x",
    "@fortawesome/free-regular-svg-icons": "5.x",
    "@fortawesome/free-solid-svg-icons": "5.x",
    "firacode": "6.2.0",
    "sanitize.css": "13.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "autoprefixer": "10.4.2",
    "cssnano": "5.1.0",
    "mdsvex": "0.10.5",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.7",
    "postcss-load-config": "3.1.3",
    "postcss-viewport-height-correction": "1.1.1",
    "prettier": "2.5.1",
    "sass": "1.49.9",
    "svelte": "3.46.4",
    "svelte-awesome": "2.4.4",
    "svelte-check": "2.4.5",
    "svelte-preprocess": "4.10.4",
    "svelte-sitemap": "2.3.0",
    "tslib": "2.3.1",
    "typescript": "4.6.2",
    "vite-plugin-iso-import": "0.1.3"
  },
  "scripts": {
    "build": "run-s build:kit build:404 build:sitemap build:pretty",
    "build:404": "mv build/404/index.html build/404.html && rmdir build/404",
    "build:kit": "svelte-kit build",
    "build:old": "mkdir -p static/old && run-p 'build:old:*'",
    "build:old:v0": "cp -r old/v0 static/old/v0",
    "build:old:v1": "npm run -w old/v1 build && cp -r old/v1/public static/old/v1",
    "build:pretty": "prettier -w 'build/**/*.html'",
    "build:sitemap": "svelte-sitemap --trailing-slashes -d https://d3bu.net -i '404.html' -i 'inner/**'",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "svelte-kit dev",
    "dev-host": "svelte-kit dev --host",
    "preview": "svelte-kit preview",
    "preview-host": "svelte-kit preview --host"
  },
  "type": "module",
  "workspaces": [
    "old/v1"
  ]
}
