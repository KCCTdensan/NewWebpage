{
  "name": "d3bu.net",
  "version": "2.0.0",
  "dependencies": {
    "sanitize.css": "13.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@themesberg/flowbite": "1.3.0",
    "autoprefixer": "10.4.2",
    "cssnano": "5.0.17",
    "mdsvex": "0.10.5",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.6",
    "postcss-load-config": "3.1.1",
    "postcss-viewport-height-correction": "1.1.1",
    "prettier": "2.5.1",
    "sass": "1.49.7",
    "svelte": "3.46.4",
    "svelte-awesome": "2.4.4",
    "svelte-preprocess": "4.10.2",
    "svelte-sitemap": "2.3.0",
    "tailwindcss": "3.0.19",
    "tslib": "2.3.1",
    "typescript": "4.5.5",
    "vite-plugin-iso-import": "0.1.3"
  },
  "scripts": {
    "build": "run-s build:kit build:404 build:sitemap build:pretty",
    "build:404": "mv build/404/index.html build/404.html && rmdir build/404",
    "build:kit": "svelte-kit build",
    "build:pretty": "prettier -w 'build/**/*.html'",
    "build:sitemap": "svelte-sitemap --trailing-slashes -d https://d3bu.net -i 'inner/**'",
    "dev": "svelte-kit dev",
    "dev-host": "svelte-kit dev --host",
    "preview": "svelte-kit preview",
    "preview-host": "svelte-kit preview --host"
  },
  "type": "module"
}
