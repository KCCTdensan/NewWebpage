{
  "name": "d3bu.net",
  "version": "2.1.1",
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "5.x",
    "@fortawesome/free-regular-svg-icons": "5.x",
    "@fortawesome/free-solid-svg-icons": "5.x",
    "katex": "0.16.0",
    "sanitize.css": "13.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "next",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "autoprefixer": "10.4.7",
    "cssnano": "5.1.12",
    "dotenv-cli": "6.0.0",
    "mdsvex": "0.10.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "postcss-load-config": "4.0.1",
    "postcss-viewport-height-correction": "1.1.1",
    "prettier": "2.7.1",
    "rehype-katex-svelte": "1.1.2",
    "remark-math": "2",
    "sass": "1.53.0",
    "svelte": "3.49.0",
    "svelte-awesome": "3.0.0",
    "svelte-check": "2.8.0",
    "svelte-preprocess": "4.10.7",
    "svelte-sitemap": "2.5.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vite": "2.9.14"
  },
  "scripts": {
    "build": "dotenv run-s build:kit build:404 build:sitemap build:pretty",
    "build:404": "mv build/404/index.html build/404.html && rmdir build/404",
    "build:kit": "vite build",
    "build:old": "mkdir -p static/old && run-p 'build:old:*'",
    "build:old:v0": "cp -r old/v0 static/old/v0 && cp -r old/v0-blog static/old/v0/Blog",
    "build:old:v1": "npm run -w old/v1 build && cp -r old/v1/public static/old/v1",
    "build:pretty": "prettier -w 'build/**/*.html'",
    "build:sitemap": "svelte-sitemap -d https://d3bu.net -i '404.html' -i 'inner/**'",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check-watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "dotenv vite dev",
    "dev-host": "dotenv vite dev --host",
    "preview": "dotenv vite preview",
    "preview-host": "dotenv vite preview --host",
    "ssr:build": "SSR='true' run-s build:kit",
    "ssr:start": "node build"
  },
  "type": "module",
  "workspaces": [
    "old/v1"
  ]
}
