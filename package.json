{
  "name": "d3bu.net",
  "version": "2.0.0",
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@themesberg/flowbite": "1.2.0",
    "autoprefixer": "10.4.1",
    "cssnano": "5.0.14",
    "mdsvex": "0.9.8",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.5",
    "postcss-load-config": "3.1.0",
    "postcss-viewport-height-correction": "1.1.1",
    "prettier": "2.5.1",
    "svelte": "3.44.3",
    "svelte-awesome": "2.4.2",
    "svelte-preprocess": "4.10.1",
    "svelte-sitemap": "2.1.3",
    "tailwindcss": "3.0.8",
    "tslib": "2.3.1",
    "typescript": "4.5.4",
    "vite-plugin-iso-import": "0.1.3"
  },
  "scripts": {
    "build": "svelte-kit build",
    "dev": "svelte-kit dev",
    "dev-host": "svelte-kit dev --host",
    "postbuild": "run-s postbuild:404 postbuild:sitemap postbuild:pretty",
    "postbuild:404": "mv build/404/index.html build/404.html && rmdir build/404",
    "postbuild:pretty": "prettier -w 'build/**/*.html'",
    "postbuild:sitemap": "svelte-sitemap --trailing-slashes -d https://d3bu.net -i 'inner/**'",
    "preview": "svelte-kit preview",
    "preview-host": "svelte-kit preview --host"
  },
  "type": "module"
}
